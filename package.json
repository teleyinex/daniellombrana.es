    "front-matter": "^3.0.1",
    "gm": "^1.23.1",
    "imports-loader": "^0.8.0",
    "markdown-it": "^8.4.2",
    "markdown-it-classy": "^0.2.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-to-json": "^0.5.1",
    "node-dir": "^0.1.17",
